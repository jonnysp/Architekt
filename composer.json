{
    "name": "jonnysp/architekt",
    "type": "contao-bundle",
    "description": "Adds Listing tools for Architekts to Contao 4",
    "keywords":["architekt","contao"],
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Jonny Spitzner",
            "homepage": "https://github.com/jonnysp"
        }
    ],
    "require": {
        "php": "^7.1",
        "contao/core-bundle": "^4.4"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Jonnysp\\Architekt\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "contao-manager-plugin": "Jonnysp\\Architekt\\ContaoManager\\Plugin"
    },
    "support": {
        "issues": "https://github.com/jonnysp/Architekt/issues",
        "source": "https://github.com/jonnysp/Architekt"
    }
}
